<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <a href="/profile">Profile</a>
    <a href="/logout">Logout</a>
    <%if(users.length>0){%>
    <p>User list:</p>
    <table>
      <tr>
        <th>Username</th>
        <th>Email</th>
        <th>Password</th>
        <th>Role</th>
      </tr>
      <%users.forEach(user=>{%>
      <tr>
        <td><%=user.username%></td>
        <td><%=user.email%></td>
        <td><%=user.password%></td>
        <td><%=user.role%></td>
      </tr>
      <%})%>
    </table>
    <%}%>
  </body>
</html>
