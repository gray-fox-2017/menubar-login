<%- include ../partials/header %>
  <body>
    <h1> Edit Idea </h1>
    <form action="/idea/update/<%= idea.id %>" method="post">
      <h4> Idea </h4>
      <input type="text" name="idea" value="<%= idea.idea %>">
      <h4> Note </h4>
      <input type="text" name="notes" value="<%= idea.notes %>">
      <button type="submit">Submit</button>
    </form>
  </body>
</html>